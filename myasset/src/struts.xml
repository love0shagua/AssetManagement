<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<!-- <package name="login" extends="struts-default" namespace="/login"> -->
<constant name="struts.objectFactory" value="spring"/>
			<package name="struts2" extends="struts-default" >
			
			<action name="userLogin" class="userLogin" method="userLogin">
				<result name="ok">/adduser.jsp</result> 
				<result name="no">/login.jsp</result>
			</action>
			
			<action name="addUser" class="com.action.AddUserAction" method="addUser">
				<result name="add">/index.jsp</result> 
	
			</action>
		
		</package>
</struts>    
